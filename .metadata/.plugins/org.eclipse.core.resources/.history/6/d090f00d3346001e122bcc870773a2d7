package day10;

public class CardMain {

	public static void main(String[] args) {
		CardPack cp= new CardPack();
		int index =0;
		for(int i = 0; i<cp.getPack().length; i ++) {
			cp.getPack()[i].print();
			index++;
			if(index % 13 ==0 ) {
				System.out.println();
			}
		} 
		//4줄씩 13개로 찍기
		
		
		
		}
		

	}


