package day09;

public class 과제 {

	public static void main(String[] args) {
		//클래스 객체 생성(2명)
		//값을 입력 후 정보출력
		

		Point p= new Point();
		p.set이름();
		p.set나이();
		p.set전화번호(0);
		p.set주소()
		p.print();
		
		
		
	}

}


//클래스 생성
//학생 클래스 생성
//이름, 나이, 전화번호, 주소
//출력
//이름 (나이) 전화번호

class point{
	
	
	

private int 이름;
private int 나이;
private int 전화번호;
private int 주소;

public void print() {
	System.out.println("("+ 이름 + ","+나이+ ","+전화번호+","+주소 + ")");
	
	public int get() {
		return 이름;
	}
	public int get() {
		return 나이;
	}
	public int get () {
		return 전화번호;
	}
	public int get() {
		return 주소;
	}
	public void set(int 이름) {
		this.이름=이름;
	}
	public void set(int 나이) {
		this.나이=나이;
	}
	public void set(int 전화번호) {
		this.전화번호=전화번호;
	}
	public void set(int 주소) {
		this.주소=주쇠
	}
	
	
	
	
}
}